{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],

  "crons": [

    {
      "path": "/v.1/api/1",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/v.1/api/3",
      "schedule": "0 0 * * WED"
    },
    {
      "path": "/v.1/api/2",
      "schedule": "* * */2 * *"
    }
  ]
}
